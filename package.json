{
  "name": "anilibrix",
  "version": "1.1.51",
  "author": "AniLibria",
  "description": "AniLibrix - AniLibria desktop app",
  "license": null,
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/PavelShar/anilibria"
  },
  "scripts": {
    "build": "node .electron-vue/build.js",
    "dist": "electron-builder -mwl",
    "dev": "node .electron-vue/dev-runner.js",
    "release": "yarn build && yarn dist",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src"
  },
  "build": {
    "productName": "AniLibrix",
    "artifactName": "${productName}-${os}-${version}.${ext}",
    "appId": "com.anilibria.anilibrix",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/electron/**/*",
      "build/icons/**/*"
    ],
    "publish": {
      "provider": "github"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "build/icons/app/anilibria.icns"
    },
    "win": {
      "icon": "build/icons/app/anilibria.ico",
      "target": [
        "portable"
      ]
    },
    "linux": {
      "icon": "build/icons/app"
    }
  },
  "dependencies": {
    "@mdi/font": "^4.8.95",
    "@sentry/browser": "^5.15.5",
    "@sentry/electron": "1.3.0",
    "@sentry/integrations": "^5.15.5",
    "axios": "^0.19.2",
    "axios-https-proxy-fix": "^0.17.1",
    "electron-promise-ipc": "^2.0.1",
    "electron-updater": "^4.2.2",
    "form-data": "^3.0.0",
    "fuse.js": "^3.4.6",
    "hls.js": "^0.13.1",
    "https-proxy-agent": "^5.0.0",
    "jikan-node": "^1.2.0",
    "lethargy": "^1.0.9",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "object-hash": "^2.0.3",
    "pac-resolver": "^4.0.0",
    "parse-torrent": "^7.0.1",
    "plyr": "^3.5.7-beta.0",
    "screenfull": "^5.0.2",
    "set-cookie-parser": "^2.4.3",
    "string-strip-html": "^4.3.16",
    "uuid": "^7.0.3",
    "vue": "^2.5.16",
    "vue-clamp": "^0.2.2",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vue-toasted": "^1.1.27",
    "vuetify": "^2.2.8",
    "vuex": "^3.0.1",
    "vuex-electron": "MaverickMartyn/vuex-electron",
    "webtorrent": "^0.107.17"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "ajv": "^6.5.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "deepmerge": "^4.2.2",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^8.2.1",
    "electron-builder": "^22.3.2",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuetify-loader": "^1.4.3",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
